<%= form_for [current_user, @address] do |f| %> 

  <%= label_tag(:street_address, "Your address") %> 
  <%= f.text_field :street_address, :placeholder => '1234 Market St.' %>
  <%= label_tag(:unit, "Unit") %> 
  <%= f.text_field :unit, :placeholder => "301 (optional)" %> 
  <%= f.submit %>  

<% end %> 